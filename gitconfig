[core]
  excludesfile = ~/.gitignore_global
[format]
  pretty = oneline
[credential]
  helper = osxkeychain
[alias]
  st = status
  sts = status -s
  co = checkout
  exclude = !sh -c 'echo "$1" >> .git/info/exclude' -
[color]
  branch = auto
  diff = auto
  status = auto
  showbranch = auto
  ui = true
[include]
  path = ~/.gitconfig.local